/* 搜索框 */
page{width: 100%;overflow-x:hidden ;}
.top-search{
  /* margin-top: 20rpx; */
  padding: 0rpx 28rpx 0rpx 28rpx;
  line-height: 56rpx;
  box-sizing: border-box;
}
.index-search-box{padding: 0 !important;border: 0px;}
/* 通用 */
.van-ellipsis{overflow: inherit !important;flex-basis: auto !important;}
.cate-wrap{background-color: #FFFFFF;}
.bgdark,.bgdark .van-tabs__scroll,.bgdark .van-tab,.bgdark .van-dropdown-menu  {
  color: #fff;
  background-color: #A49E83;
}
.bgff{background-color: #fff;}
.loadtxt{padding: 20rpx;}
.types-wrap {border: 0px !important;}
.types-wrap .van-tab.van-tab--active{color: #fff;}
.types-wrap .van-hairline--top-bottom:after{border-width:0px !important;}
.types-wrap .van-tab .van-tabs__line{background-color: #fff;}
.types-wrap .sel-wrap .van-dropdown-menu__title:after{
  background-image:url('data:image/png;base64,');
  background-size: cover;
}
.icons{width: 30rpx;height: 34rpx;vertical-align: -5rpx;margin-left: 26rpx;margin-right: 26rpx;}
.main-wrap{padding:0rpx 0rpx 20rpx ;box-sizing: border-box;}
.main-block{padding-top:44rpx;box-sizing: border-box;}
.main-wrap .seli-wrap{padding:4rpx 0 16rpx;box-sizing: border-box;}
.main-wrap .seli-wrap .van-col{margin-top: 26rpx;}
.main-wrap .seli-block{
  width:156rpx;
  height:56rpx;
  line-height:56rpx;
  border-radius:56rpx;
  border:1px solid rgba(224,224,224,1);
}
.main-wrap .seli-block2{
  line-height:56rpx;
  border-radius:56rpx;
  border:0px;
  background-color: #F5F5F5;
}
.main-wrap .seli-block2.active{background-color: #FDE8E8;}
.main-wrap .seli-block2.active-icon{position: relative;}
.main-wrap .seli-block2.active-icon:before{position: absolute;content: "";left:0;top: 40%; }
.tc-tit{padding-top: 50rpx;padding-bottom: 30rpx;box-sizing: border-box;}
.types-wrap .sel-wrap .type-list-row ul li{color:#D3C792; border-radius: 12rpx;}
.types-wrap .sel-wrap .type-list-row ul li.tactive{color: white;background-color:#D3C792;}
.swiper-wrap,.swiper-wrap swiper,.swiper-wrap swiper-item,.swiper-wrap swiper-item image{width: 100%;height: 280rpx;}
.sxwrap{height: 72rpx;line-height: 72rpx;position: relative;overflow: hidden;box-sizing: border-box;}
.sxwrap .sc{color: #1D1D1D;font-size: 22rpx;position: relative;display: inline-block;text-align: center;}
.sxwrap .sc{padding-right:28rpx ;box-sizing: border-box;}
.sxwrap .sc{padding-left:16rpx;box-sizing: border-box;}
.sxwrap .sc:after{content: "/";position: absolute;top: 0rpx; right: 0;transform: rotate(13deg); color: #7C8278;}
.sgwrap .wx-swiper-dots.wx-swiper-dots-horizontal{
  margin-bottom: 30rpx;
}
.suwrap{padding: 50rpx 35rpx ; box-sizing: border-box;}
.sgwrap{width:100%;height:275rpx}
.sgwrap .gswip-img{width:100%;height:275rpx}
.cate-wrap .types-wrap .van-tab.van-tab--active {font-size: 28rpx;}
.van-tabs__line {background-color:#fff !important;}
.hidclass{height:100vh;overflow:hidden;}

.index-grid-b .van-grid-item__content--surround:after {border:0px;}
.box{
  position: relative;
  width: 100%;
  height: 80rpx;
  overflow: hidden;
}

.txt{
  white-space: nowrap;
}
.flex-box{
  position: absolute;
  top: 0;
}

.flex-box{
  display: flex;
  flex-wrap: nowrap;
  justify-content: flex-start;
}
.van-cell{
  border-bottom:1rpx solid #f0f0f0
}  
swiper-container{
  position: relative;
}
.swiper-container .dots{
  position: absolute;
 margin-top:-30rpx;
  width: 80vw;
  /* bottom: 20rpx; */
  display: flex;
  justify-content: center;
  background-color:rgba(255, 255, 255, 0.40);
  margin-left: 10vw;
}
.swiper-container .dots .dot{
  /* margin: 0 8rpx; */
  width: 100%;
  height: 8rpx;
  /* background: red; */
  /* border-radius: 8rpx; */
  transition: all .6s;
}
.swiper-container .dots .dot.active{
  width: 100%;
  background:rgba(255, 255, 255, 0.97);
}
.msg {
      display: flex;
    align-items: center;
    justify-content: center;
  border-radius: 50rpx;
  top: 11rpx;
  background-color: red;
  margin-left: 35rpx;
  position: absolute;
  color: white;
  min-width: 30rpx;
  height: 30rpx; 
  padding: 0 5rpx;
}
